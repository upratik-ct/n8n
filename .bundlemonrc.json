// This JSON object defines configuration settings for a tool or process related to a frontend editor
// UI project. Here is a breakdown of the key-value pairs within the object:
// This part of the JSON object is defining configuration settings related to the report output for a
// tool or process in a frontend editor UI project.
{
	"baseDir": "packages/frontend/editor-ui/dist",
	"defaultCompression": "gzip",
	"reportOutput": [
		[
			"github",
			{
				"checkRun": true,
				"commitStatus": "off",
				"prComment": true
			}
		]
	],
// The `files` and `groups` sections in the JSON object are defining specific configurations related to
// file paths and group names for the frontend editor UI project's report output.
	"files": [
		{
			"path": "*.wasm",
			"friendlyName": "WASM Dependencies"
		}
	],
	"groups": [
		{
			"groupName": "Editor UI - Total JS Size",
			"path": "**/*.js"
		},
		{
			"groupName": "Editor UI - Total CSS Size",
			"path": "**/*.css"
		}
	]
}
